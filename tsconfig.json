{
  "extends": "./lib/tsconfig.json",
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "allowSyntheticDefaultImports": true,
    "erasableSyntaxOnly": true,
    "noUncheckedIndexedAccess": false,
    "noImplicitReturns": false,
    "jsx": "preserve",
    "lib": ["es2022", "webworker", "dom.iterable"],
    "types": ["node", "@cloudflare/workers-types"],
    "paths": {
      "#nitro-internal-virtual/*": ["./src/types/virtual/*.ts"],
      "nitro/~internal/runtime/*": ["./src/runtime/internal/*.ts"]
    }
  },
  "exclude": [
    "examples/import-alias/**",
    "test/fixture/server/routes/jsx.tsx",
    "examples/vite-ssr-solid/src/entry-server.tsx",
    "examples/vite-ssr-solid/src/entry-client.tsx"
  ]
}
